<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">
        <% if (typeof loggedin !== 'undefined' && loggedin) { %>
            <% if (typeof accountData !== 'undefined' && accountData) { %>
                <a href="/account/" title="Account Management">Welcome <%= accountData.account_firstname %>  <%= accountData.account_type %></a> | 
            <% } %>
            <a href="/account/logout" title="Logout">Logout</a>
        <% } else { %>
            <a href="/account/login" title="Click to log in">My Account</a>
        <% } %>
    </div>
</header>